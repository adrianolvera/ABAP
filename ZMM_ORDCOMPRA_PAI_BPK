*&---------------------------------------------------------------------*
*&  Include           ZMM_ORDCOMPRA_PAI_BPK
*&----------------------------------------------------------------------*

*&--------------------------------------------------------------------------*
*&      Module  USER_COMMAND_0110  INPUT
*&---------------------------------------------------------------------*
MODULE USER_COMMAND_0110 INPUT.
  SAVE_OK = OK_CODE.
  CLEAR:OK_CODE.
  CASE SAVE_OK.
    WHEN 'BACK'.
      LEAVE TO SCREEN 0.
    WHEN 'CANCEL'.
      LEAVE TO SCREEN 0.
    WHEN 'EXIT'.
      LEAVE PROGRAM.
    WHEN OTHERS.
  ENDCASE.
ENDMODULE.                     " USER_COMMAND_0110  INPUT
